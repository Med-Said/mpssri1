package partie2;

public class TestFact {
	public static void main(String[] args) {
		
		System.out.println(fact(3));;
		
	}
	
	public static int fact(int n){
		if(n ==0 || n ==1){
			//System.out.println(n+"! = " + n);
			return n;
		}	
		else {
			//System.out.println(n+"! = " + n*(fact(n-1)));
			return n*(fact(n-1));
		}				
	}
}
