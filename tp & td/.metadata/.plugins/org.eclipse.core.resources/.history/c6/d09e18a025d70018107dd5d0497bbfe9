package expose;

public class Enseignant extends Personne{
	private String matiere;
	private short nbrClasse;
	private short nbrHoraire;
	
	public Enseignant(String nom, String prenom){
		super(nom, prenom);
	}
	
	public Enseignant(String nom, String prenom, short nbrClasse, short nbrHoraire){
		super(nom,prenom);
		this.nbrClasse = nbrClasse;
		this.nbrHoraire = nbrHoraire;
	}
	
}
