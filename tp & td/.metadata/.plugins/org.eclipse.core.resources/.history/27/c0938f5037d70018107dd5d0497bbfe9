package tp19;

public class Main {
	public static void main(String[] args) {
		//suite 1
		Personne p1 = new Personne("Malake",21);
		System.out.println(p1.toString());
		
//		
		Skieur sk1 = new Skieur("Malake",21,true);
		Skieur sk2 = new Skieur("Rim",21,true);
		
		System.out.println(sk1.toString());
		System.out.println(sk2.toString());
//		
		
	}
}
