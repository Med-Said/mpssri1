package expose;

public class MainClass {

	public static void main(String[] args) {
		
		
		//execution 1
		Personne p1 = new Personne("Aloui", "Rim");
		System.out.println(p1.decrit());
		
		Etudiant e1 = new Etudiant("Thabti", "Sana");
		System.out.println(e1.decrit());
		
		Enseignant en1 = new Enseignant("Maghrbei", "Wafa");
		System.out.println(en1.decrit());
		
		//execution 2
	}
}
