package expose;

public class Enseignant extends Personne{
	private String matiere;
	private short nbrClasse;
	private short nbrHoraire;
	
	public Enseignant(String nom, String prenom){
		super(nom, prenom);
	}
	
	public Enseignant(String nom, String prenom, String matiere, short nbrClasse, short nbrHoraire){
		super(nom,prenom);
		this.matiere = matiere;
		this.nbrClasse = nbrClasse;
		this.nbrHoraire = nbrHoraire;
	}
	
	public String decrit(){
		
		return "\tEnseignant\n" +  super.decrit();
		
	}
	
}
